#!/bin/bash

# this script exists to test different serial port possibilities for USB[0-1]

/home/$USER/mav/mavconn/build/bin/mavconn-bridge-serial -p /dev/ttyUSB0 --pc2serial || /home/$USER/mav/mavconn/build/bin/mavconn-bridge-serial -p /dev/ttyUSB1 --pc2serial || /home/$USER/mav/mavconn/build/bin/mavconn-bridge-serial -p /dev/ttyUSB2 --pc2serial
